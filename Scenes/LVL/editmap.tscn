script/source = "extends WindowDialog

var time = 12 #Стартовое время
var time_change = true #Может ли время меняться
var time_event = 10 #Раз во сколько секунд время меняеться

var can_be_saved = true #Может ли карта сохраняться
var edit_save = false #Можно ли редактировать сохранения

var this_is_save = false #Эта копия являеться сохранением

var number_of_clutches = 0

var spavn_y = 0
var spavn_x = 0
var re_spavn = false
var specter = false

var taking_away_hunger = 12 #Раз во сколько секунд голод и здоровье меняеться
var dehumidification_frequency = 10 #Раз во сколько секунд бодрость меняеться

func get_setting_map():
	print(\"get_setting_map сработал\")
	$ScrollContainer/VBoxContainer/time/CheckBox.value = time
	$ScrollContainer/VBoxContainer/time_change/CheckBox.pressed = time_change
	$ScrollContainer/VBoxContainer/time_event/CheckBox.value = time_event
	
	$ScrollContainer/VBoxContainer/can_be_saved/CheckBox.pressed = can_be_saved
	$ScrollContainer/VBoxContainer/edit_save/CheckBox.pressed = edit_save
	
	$ScrollContainer/VBoxContainer/number_of_clutches/CheckBox.value = number_of_clutches
	
	$ScrollContainer/VBoxContainer/spavn_x/CheckBox.value = spavn_x
	$ScrollContainer/VBoxContainer/spavn_y/CheckBox.value = spavn_y
	$ScrollContainer/VBoxContainer/re_spavn/CheckBox.pressed = re_spavn
	$ScrollContainer/VBoxContainer/specter/CheckBox.pressed = specter
	
	$ScrollContainer/VBoxContainer/taking_away/CheckBox.value = taking_away_hunger
	$ScrollContainer/VBoxContainer/dehumidification_frequency/CheckBox.value = dehumidification_frequency

func set_setting_map():
	print(\"set_setting_map сработал\")
	time = $ScrollContainer/VBoxContainer/time/CheckBox.value
	time_change = $ScrollContainer/VBoxContainer/time_change/CheckBox.pressed
	time_event = $ScrollContainer/VBoxContainer/time_event/CheckBox.value
	
	can_be_saved = $ScrollContainer/VBoxContainer/can_be_saved/CheckBox.pressed
	edit_save = $ScrollContainer/VBoxContainer/edit_save/CheckBox.pressed
	
	number_of_clutches = $ScrollContainer/VBoxContainer/number_of_clutches/CheckBox.value
	
	spavn_x = $ScrollContainer/VBoxContainer/spavn_x/CheckBox.value
	spavn_y = $ScrollContainer/VBoxContainer/spavn_y/CheckBox.value
	re_spavn = $ScrollContainer/VBoxContainer/re_spavn/CheckBox.pressed
	specter = $ScrollContainer/VBoxContainer/specter/CheckBox.pressed
	
	taking_away_hunger = $ScrollContainer/VBoxContainer/taking_away/CheckBox.value
	dehumidification_frequency = $ScrollContainer/VBoxContainer/dehumidification_frequency/CheckBox.value
	
	$ScrollContainer/VBoxContainer/edit_save/CheckBox.disabled = not can_be_saved
	
	$ScrollContainer/VBoxContainer/time_event/CheckBox.editable = time_change

func popup_hide():
	set_setting_map()

func about_to_show():
	get_setting_map()

func changed():
	set_setting_map()

func CheckBox_pressed():
	set_setting_map()
"

