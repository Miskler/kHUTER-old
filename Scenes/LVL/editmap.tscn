	g.creation_position_x = DataScene.spavn_x
	g.creation_position_y = DataScene.spavn_y
	
	g.taking_away_hunger = DataScene.taking_away_hunger
	g.dehumidification_frequency = DataScene.dehumidification_frequency
	
	get_node(\"/root/editmap/Node\").add_child(g)
	g.owner = get_node(\"/root/editmap/Node\")
	
	print(path)
	
	get_node(\"../../../Node\").get_script().source_code = get_node(\"../../../NodeScript\").get_script().source_code
	
	s(path)

func s(pu): #Сохраняет карту!!!
	Nd = get_node(\"../../../Node\")
	saveNode = Nd
	var result = scene.pack(saveNode)
	if result == OK:
		var error = ResourceSaver.save(pu + \"_HUTER.tscn\", scene)
		if error != OK:
			push_error(\"Произошла ошибка при сохранении сцены на диск.\")
	
	get_node(\"/root/editmap/Node/SettingData\").queue_free()

