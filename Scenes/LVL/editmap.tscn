script/source = "extends Camera2D

var speed = 10
var zoom_speed = 0.1

func _process(_delta):
	if $CanvasLayer/Control/FileDialog.visible == false and $CanvasLayer/Control/FileDialog2.visible == false and $CanvasLayer/Control/edit.visible == false:
		if Input.is_action_pressed(\"+\") and self.zoom.x < 10:
			self.zoom.x += zoom_speed
			self.zoom.y += zoom_speed
			print(\"Зум камеры: \", zoom.x)
		if Input.is_action_pressed(\"-\") and self.zoom.x > 1.1:
			self.zoom.x -= zoom_speed
			self.zoom.y -= zoom_speed
			print(\"Зум камеры: \", zoom.x)
		
		
		if Input.is_action_pressed(\"w\"):
			position.y -= speed * zoom.x/2
		elif Input.is_action_pressed(\"s\"):
			position.y += speed * zoom.x/2
		
		if Input.is_action_pressed(\"d\"):
			position.x += speed * zoom.x/2
		elif Input.is_action_pressed(\"a\"):
			position.x -= speed * zoom.x/2
"

