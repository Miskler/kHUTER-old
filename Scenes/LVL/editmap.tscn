script/source = "extends ItemList

onready var root_node = get_node(\"/root/editmap/Node\")
var tree = self
var nodes = []
var d = 1 #Ребёнок над которым скрипт проводит операцию
var select = []
var status = false

var j
var h

func _ready():
	start()

func _viever():
	var info = $\"../Control2\"
	if select.size() > 0 and status == true:
		j = root_node.get_child(select[0])
		h = j.get_class()
		info.get_node(\"Label\").text = h
		if h == \"Timer\":
			print(\"паааб\")
			info.get_node(\"Timer\").show()
			info.get_node(\"Timer/SpinBox\").value = j.wait_time
			info.get_node(\"Timer/CheckBox2\").pressed = j.one_shot
			info.get_node(\"Timer/LineEdit\").text = \"$\" + str(j.name) + \".connect(\\\"timeout\\\", self, \\\"НАЗВАНИЕ_ВАШЕЙ_ФУНКЦИИ\\\")\"
		else:
			info.get_node(\"Timer\").hide()
	else:
		info.get_node(\"Label\").text = \"Неизвестная нода\"

func start():
	root_node = get_node(\"/root/editmap/Node\")
	select = get_selected_items()
	tree.clear()
	nodes = []
	if root_node.get_child_count() > 0:
		d = 1 #Ребёнок над которым скрипт проводит операцию
		for _i in range(root_node.get_child_count()): #Объявляем цикл, количевство вызовов которого зависит от количевства детей
			nodes.append(tree.add_item(root_node.get_child(d - 1).get_name())) #Создаём объект в дереве и добавляем в массив
			d += 1
	if select.size() > 0 and nodes.size() >= select.size():
		var f = select[0]
		print(f)
		select(f)
		_viever()

func node_up():
	if select.size() > 0:
		var f = select[0]
		var n = root_node.get_child(f)
		if (n.get_index()) > 0:
			root_node.move_child(n, f - 1)
			select(n.get_index())
			start()

func event():
	start()

func node_bottom():
	if select.size() > 0:
		var f = select[0]
		var n = root_node.get_child(f)
		if (n.get_index()) < nodes.size():
			root_node.move_child(n, f + 1)
			select(n.get_index())
			start()

func event_pressed():
	start()

func changed(value):
	if value > 0:
		$event.wait_time = value
		$event.start()
	else:
		$event.stop()

func node_del():
	if select.size() > 0:
		var f = select[0]
		var n = root_node.get_child(f)
		n.queue_free()
		start()

func full_setting_pressed():
	status = not status
	if status == false:
		rect_size.y = 105
		$\"../Control2\".hide()
	else:
		rect_size.y = 440
		$\"../Control2\".show()

func cent():
	if select.size() > 0:
		var f = select[0]
		var n = root_node.get_child(f)
		get_node(\"/root/editmap/Camera2D\").global_position = n.global_position

func timer_make():
	var timer = Timer.new()
	root_node.add_child(timer)
	timer.name = \"Timer\"


func value_changed_timer(value):
	j.wait_time = value

func one_shot(value):
	j.one_shot = value
