	var g = load(\"res://Sceni/LVL/SettingData.res\").instance()
	
	g.time = DataScene.time
	g.time_change = DataScene.time_change
	g.time_event = DataScene.time_event
	
	g.can_be_saved = DataScene.can_be_saved
