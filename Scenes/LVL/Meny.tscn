script/source = "extends Control

var servers = {}

const way = \"user://SaveGame/servers.json\"
var file = File.new()
var peer = NetworkedMultiplayerENet.new()

func _ready():
	$ItemList.clear()
	get_tree().connect(\"connection_failed\", self, \"failed\")
	get_tree().connect(\"connected_to_server\", self, \"go\")
	if !file.file_exists(way): 
		file.file_exists(way)
	else:
		file.open(way, File.READ)
		servers = parse_json(file.get_as_text())
		file.close()
		respavn()

func _exit_tree():
	file.open(way, File.WRITE)
	file.store_line(to_json(servers))
	file.close()

func respavn():
	_exit_tree()
	$ItemList.clear()
	for i in range(servers.size()):
		var id = servers.keys()[i]
		$ItemList.add_item(servers[id])
	if servers.size() > 0:
		$ItemList.select(0)

func cansel():
	$\".\".hide()

func adres():
	$connect.show()

func no():
	$add_server.hide()
	$connect.hide()

func add_server():
	$add_server.show()
	$add_server/Label.text = \"Добавление сервера\"
	$add_server/LineEdit.text = \"Сервер HUTER\"
	$add_server/LineEdit2.text = \"\"

func edit_server():
	if $ItemList.get_selected_items().size() > 0:
		$add_server.show()
		$add_server/Label.text = \"Изменение сервера\"
		var keys = servers.keys()
		$add_server/LineEdit.text = servers[keys[$ItemList.get_selected_items()[0]]]
		$add_server/LineEdit2.text = keys[$ItemList.get_selected_items()[0]]

func yes():
	$add_server.hide()
	if $add_server/LineEdit.text != \"\" and $add_server/LineEdit2.text != \"\":
		servers[$add_server/LineEdit2.text] = $add_server/LineEdit.text
		respavn()

func del_server():
	if $ItemList.get_selected_items().size() > 0:
		servers.erase(servers.keys()[$ItemList.get_selected_items()[0]])
		respavn()

func connect_select_server():
	if $ItemList.get_selected_items().size() > 0:
		peer = NetworkedMultiplayerENet.new()
		
		peer.create_client(servers.keys()[$ItemList.get_selected_items()[0]], G.game_settings[\"port\"])
		get_tree().network_peer = peer
		$\"../Control3\".show()
		printt(get_tree().network_peer, servers.keys()[$ItemList.get_selected_items()[0]], G.game_settings[\"port\"])

func connect_server():
	$connect.hide()
	if $connect/LineEdit.text != \"\":
		peer = NetworkedMultiplayerENet.new()
		
		peer.create_client($connect/LineEdit.text, G.game_settings[\"port\"])
		get_tree().network_peer = peer
		$\"../Control3\".show()
		printt(get_tree().network_peer, $connect/LineEdit.text, G.game_settings[\"port\"])

func failed():
	print(\"failed\")
	get_tree().network_peer = null
	$\"../Control3\".hide()
	$\"../AcceptDialog\".popup_centered()

func go():
	print(\"go\")
	get_tree().change_scene(\"res://Sceni/rootGame.tscn\")

func stop():
	$\"../Control3\".hide()
	get_tree().network_peer = null
"

