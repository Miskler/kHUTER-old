script/source = "extends Control

func _ready():
	add_element()

func add_element():
	var dir = Directory.new()
	if dir.open(\"user://SaveGame/Worlds\") == OK:
		dir.list_dir_begin()
		var file_name = dir.get_next()
		while file_name != \"\":
			var file = File.new()
			if file_name != \".\" and dir.current_is_dir() and file.file_exists(\"user://SaveGame/Worlds/\" + file_name + \"/map.huter\"):
				print(1)
				var item = get_node(\"ScrollContainer/VBoxContainer/S*ample\").duplicate()
				print(item)
				
				item.name = file_name
				item.get_node(\"Label\").text = file_name
				
				var image = Image.new()
				var err = image.load(\"user://SaveGame/Worlds/\" + file_name + \"/screenshot.png\")
				if err == OK:
					item.get_node(\"TextureRect\").texture = ImageTexture.new()
					item.get_node(\"TextureRect\").texture.create_from_image(image, 0)
				else:
					item.get_node(\"TextureRect\").texture = load(\"res://Tecture/Modeli/no_texture.png\")
				
				if file.open(\"user://SaveGame/Worlds/\" + file_name + \"/data.json\", File.READ) == OK:
					file.open(\"user://SaveGame/Worlds/\" + file_name + \"/data.json\", File.READ)
					var data = str2var(file.get_as_text())
					file.close()
					item.get_node(\"Label2\").text = \"Версия игры \" + data[0] + \", дата создания \" + str(data[2][\"day\"])+\".\"+str(data[2][\"month\"])+\".\"+str(data[2][\"year\"]) + \", автор \" + data[1]
				else:
					item.get_node(\"Label2\").text = \"Данные отцуцтвуют!\"
				
				item.show()
				get_node(\"ScrollContainer/VBoxContainer\").add_child(item)
				item.get_node(\"Button3\").connect(\"dir_load_pressed\", self, \"dir_load_game\")
				item.get_node(\"Button\").connect(\"dir_open_pressed\", self, \"dir_open\")
				item.get_node(\"Button2\").connect(\"dir_del_pressed\", self, \"dir_del_game\")
			file_name = dir.get_next()

func del_all_element():
	for i in $\"ScrollContainer/VBoxContainer\".get_children():
		if i.name != \"S*ample\":
			i.queue_free()

func list_reload():
	$\"/root/Node2D/AudioStreamPlayer2D2\".play()
	del_all_element()
	yield(get_tree(),\"idle_frame\")
	add_element()

func dir_del_game(name_save):
	$\"/root/Node2D/AudioStreamPlayer2D2\".play()
	$\"/root/Node2D/ConfirmationDialog\".window_title = name_save
	$\"/root/Node2D/ConfirmationDialog\".dialog_text = \"Вы точно хотите удалить \\\"\" + name_save + \"\\\"?\"
	$\"/root/Node2D/ConfirmationDialog\".popup_centered()

func dir_load_game(name_save):
	G.loadSCN = \"user://SaveGame/Worlds/\" + name_save + \"/map.huter\"
	$\"/root/Node2D/AudioStreamPlayer2D2\".play()
	$\"/root/Node2D/AnimationPlayer\".play(\"zatem\")

func dir_open(name_save):
	OS.shell_open(str(OS.get_user_data_dir()) + \"/SaveGame/Worlds/\" + name_save)
	$\"/root/Node2D/AudioStreamPlayer2D2\".play()

func close():
	$\"/root/Node2D/AudioStreamPlayer2D2\".play()
	self.hide()

func del_dir_accept():
	var directory = Directory.new()
	var path = str(OS.get_user_data_dir()) + \"/SaveGame/Worlds/\" + str($\"/root/Node2D/ConfirmationDialog\".window_title)
	G.remove_recursive(path)
	list_reload()

func load_scene():
	$\"/root/Node2D/AudioStreamPlayer2D2\".play()
	$FileDialog.popup_centered()

func file_selected(path):
	G.loadSCN = path
	$\"/root/Node2D/AudioStreamPlayer2D2\".play()
	$\"/root/Node2D/AnimationPlayer\".play(\"zatem\")
"

